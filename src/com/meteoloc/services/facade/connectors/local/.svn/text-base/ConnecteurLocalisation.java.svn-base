package com.meteoloc.services.facade.connectors.local;

/**
 * 
 * Interface de webservices de localisation
 * @author Mathieu DURAND - Cedric JACQUET - Yohann STREIBEL - Ewan GICQUEL
 *
 */
public interface ConnecteurLocalisation {

	/**
	 * @return execution time of reaching the service & parsing
	 */
	public abstract long getExecutionTime();
	
	public abstract String getStatus_code();

	public abstract String getStatus_request();

	public abstract String getAddress();

	public abstract String getAccuracy();

	public abstract String getCountryNameCode();

	public abstract String getCountryName();

	public abstract String getAdministrativeAreaName();

	public abstract String getSubAdministrativeAreaName();

	public abstract String getLocalityName();

	public abstract String[] getLatLonBox();

	public abstract String getCoordinates();

}